<Alloy>
	<Collection src="webNews" />
	
	<Window id='win'>
		<TableView dataCollection="webNews" dataTransform="transformFunction" dataFilter="filterFunction" onClick="eventClick3">
			<TableViewRow layout="vertical" >
				<ImageView image="{image}" top="20dp" />
				<Label text="{fullDes}" bottom="20dp" />
				<Button title="Address"  v_id="{id}" onClick="addressClick"  left="20dp"/>
			</TableViewRow>
		</TableView>
	</Window>
</Alloy>